{
  "name": "node-enviroment",
  "version": "1.0.0",
  "description": "Enviroment created to feed an API",
  "main": "./src/server.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify ./src/server.ts",
    "migrate": "npx prisma migrate dev --name init",
    "seeds": "ts-node-dev ./src/seeds/Seeds.ts",
    "clean-migrate": "npx prisma migrate reset init",
    "build": "tsc --preserveWatchOutput ./src/"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^27.4.7",
    "prettier": "^2.6.2",
    "prisma": "3.10.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "yarn": "^1.22.17"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "express": "^4.18.1",
    "express-openid-connect": "^2.7.2",
    "firebase": "^9.8.0",
    "firebase-admin": "^10.2.0",
    "mongoose": "^6.1.10",
    "multer": "^1.4.4",
    "tsc": "^2.0.4"
  }
}
