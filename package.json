{
  "name": "node-enviroment",
  "version": "1.0.0",
  "description": "Enviroment created to feed an API",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify src/server.ts",
