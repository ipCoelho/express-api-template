generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tbl_categorias {
  idCategorias      Int                 @id @unique(map: "idtblCategorias_UNIQUE") @default(autoincrement())
  nome              String              @db.VarChar(150)
  tbl_ong_categoria tbl_ong_categoria[]
}

model tbl_chat {
  idChat         Int              @id @unique(map: "idtblChat_UNIQUE") @default(autoincrement())
  nomeArquivo    String           @db.Text
  tbl_chat_login tbl_chat_login[]
}

model tbl_chat_login {
  idChatLogin Int       @id @unique(map: "idtblChatLogin_UNIQUE") @default(autoincrement())
  idLogin     Int
  idChat      Int
  tbl_chat    tbl_chat  @relation(fields: [idChat], references: [idChat], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_chat_login_tbl_chat1")
  tbl_login   tbl_login @relation(fields: [idLogin], references: [idLogin], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_chat_login_tbl_login1")

  @@index([idChat], map: "fk_tbl_chat_login_tbl_chat1_idx")
  @@index([idLogin], map: "fk_tbl_chat_login_tbl_login1_idx")
}

model tbl_comentario {
  idcomentario Int         @id @unique(map: "idtblcomentario_UNIQUE") @default(autoincrement())
  comentario   String?     @db.VarChar(300)
  createdAt    DateTime    @default(now())
  idOng        Int
  idUsuario    Int
  tbl_ong      tbl_ong     @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_comentario_tbl_ong1")
  tbl_usuario  tbl_usuario @relation(fields: [idUsuario], references: [idUsuario], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_comentario_tbl_usuario1")

  @@index([idOng], map: "fk_tbl_comentario_tbl_ong1_idx")
  @@index([idUsuario], map: "fk_tbl_comentario_tbl_usuario1_idx")
}

model tbl_contato {
  idcontato Int     @id @unique(map: "idcontato_UNIQUE") @default(autoincrement())
  numero    String? @db.VarChar(45)
  email     String? @unique(map: "email_UNIQUE") @db.VarChar(80)
  telefone  String? @db.VarChar(45)
  idOng     Int     @unique
  tbl_ong   tbl_ong @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblContato_tblOng1")

  @@index([idOng], map: "fk_tblContato_tblOng1_idx")
}

model tbl_dados_bancarios {
  idDadosBancario Int     @id @unique(map: "idDadosBancario_UNIQUE") @default(autoincrement())
  banco           String  @db.VarChar(50)
  agencia         String  @db.VarChar(10)
  conta           String  @db.VarChar(20)
  tipo            String  @db.VarChar(30)
  idOng           Int     @unique(map: "idOng_UNIQUE")
  tbl_ong         tbl_ong @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblDadosBancarios_tblOng1")
}

model tbl_endereco {
  idEndereco         Int                  @id @unique(map: "idEndereco_UNIQUE") @default(autoincrement())
  bairro             String               @db.VarChar(150)
  numero             Int
  cep                String               @db.VarChar(80)
  rua                String               @db.VarChar(80)
  complemento        String?              @db.Text
  municipio          String               @db.VarChar(80)
  idEstado           Int
  idLogin            Int                  @unique(map: "idLogin_UNIQUE")
  tbl_login          tbl_login            @relation(fields: [idLogin], references: [idLogin], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblEndereco_tblLogin1")
  tbl_estado         tbl_estado           @relation(fields: [idEstado], references: [idEstado], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblEndereco_tblEstado1")
  tbl_endereco_vagas tbl_endereco_vagas[]
  tbl_eventos        tbl_eventos[]
  tbl_login_endereco tbl_login_endereco[]



  @@index([idEstado], map: "fk_tblEndereco_tblEstado1_idx")
}

model tbl_endereco_vagas {
  idEnderecoVagas Int          @id @unique(map: "idtbl_endereco_vagas_UNIQUE") @default(autoincrement())
  idEndereco      Int
  idVagas         Int
  tbl_endereco    tbl_endereco @relation(fields: [idEndereco], references: [idEndereco], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_endereco_vagas_tbl_endereco1")
  tbl_vagas       tbl_vagas    @relation(fields: [idVagas], references: [idVagas], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_endereco_vagas_tbl_vagas1")

  @@index([idEndereco], map: "fk_tbl_endereco_vagas_tbl_endereco1_idx")
  @@index([idVagas], map: "fk_tbl_endereco_vagas_tbl_vagas1_idx")
}

model tbl_estado {
  idEstado       Int              @id @unique(map: "idEstado_UNIQUE") @default(autoincrement())
  nome           String?          @unique(map: "nome_UNIQUE") @db.VarChar(100)
  sigla          String?          @unique(map: "sigla_UNIQUE") @db.VarChar(10)
  tbl_endereco   tbl_endereco[]
  tbl_ong_estado tbl_ong_estado[]
}

model tbl_eventos {
  idEventos           Int                  @id @unique(map: "idEventos_UNIQUE") @default(autoincrement())
  titulo              String               @db.VarChar(200)
  dataHora            DateTime             @db.DateTime(0)
  objetivo            String               @db.Text
  descricao           String               @db.Text
  dataDeCriacao       DateTime             @db.Date
  idEndereco          Int
  idOng               Int
  numeroParticipantes Int?
  candidatos          Boolean              @db.TinyInt
  tbl_endereco        tbl_endereco         @relation(fields: [idEndereco], references: [idEndereco], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_eventos_tbl_endereco1")
  tbl_ong             tbl_ong              @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_eventos_tbl_ong1")
  tbl_usuario_evento  tbl_usuario_evento[]

  @@index([idEndereco], map: "fk_tbl_eventos_tbl_endereco1_idx")
  @@index([idOng], map: "fk_tbl_eventos_tbl_ong1_idx")
}

model tbl_favoritos {
  idFavoritos Int         @id @unique(map: "idFavoritos_UNIQUE") @default(autoincrement())
  idOng       Int
  idUsuario   Int
  usuario     String?     @db.VarChar(100)
  ong         String?     @db.VarChar(100)
  favoritado  Boolean     @db.TinyInt
  tbl_ong     tbl_ong     @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_favoritos_tbl_ong1")
  tbl_usuario tbl_usuario @relation(fields: [idUsuario], references: [idUsuario], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_favoritos_tbl_usuario1")

  @@index([idOng], map: "fk_tbl_favoritos_tbl_ong1_idx")
  @@index([idUsuario], map: "fk_tbl_favoritos_tbl_usuario1_idx")
}

model tbl_post_media {
  idPostMedia Int      @id @unique(map: "idtblFoto_UNIQUE") @default(autoincrement())
  titulo      String   @db.VarChar(500)
  referencia  String?  @db.VarChar(500)
  tipo        String?  @db.VarChar(50)
  url         String?  @db.VarChar(500)
  idOng       Int
  idPost      Int
  tbl_post    tbl_post @relation(fields: [idPost], references: [idPost], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_post_media_tbl_post1")
  tbl_ong     tbl_ong? @relation(fields: [idOng], references: [idOng])

  @@index([idPost], map: "fk_tbl_post_media_tbl_post1_idx")
}

model tbl_informacoes_de_contato {
  idInformacoesDeContado Int         @id @unique(map: "idInformacoesDeContado_UNIQUE") @default(autoincrement())
  numero                 String?     @db.VarChar(45)
  email                  String      @unique(map: "email_UNIQUE") @db.VarChar(100)
  telefone               String?     @db.VarChar(45)
  idUsuario              Int
  tbl_usuario            tbl_usuario @relation(fields: [idUsuario], references: [idUsuario], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblInformacoesDeContado_tblUsuario1")

  @@index([idUsuario], map: "fk_tblInformacoesDeContado_tblUsuario1_idx")
}

model tbl_login {
  idLogin            Int                  @id @unique(map: "idtbl_login_UNIQUE") @default(autoincrement())
  email              String               @unique(map: "email_UNIQUE") @db.VarChar(80)
  senha              String               @db.VarChar(80)
  tbl_chat_login     tbl_chat_login[]
  tbl_login_endereco tbl_login_endereco[]
  tbl_login_reacao   tbl_login_reacao[]
  tbl_ong            tbl_ong[]
  tbl_usuario        tbl_usuario[]
  tbl_endereco       tbl_endereco[]
}

model tbl_login_endereco {
  idLoginEndereco Int          @id @unique(map: "idtbl_login_endereco_UNIQUE") @default(autoincrement())
  idEndereco      Int
  idLogin         Int
  tbl_endereco    tbl_endereco @relation(fields: [idEndereco], references: [idEndereco], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_login_endereco_tbl_endereco1")
  tbl_login       tbl_login    @relation(fields: [idLogin], references: [idLogin], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_login_endereco_tbl_login1")

  @@index([idEndereco], map: "fk_tbl_login_endereco_tbl_endereco1_idx")
  @@index([idLogin], map: "fk_tbl_login_endereco_tbl_login1_idx")
}

model tbl_login_reacao {
  idLoginReacao Int        @id @unique(map: "idtbl_login_reacao_UNIQUE") @default(autoincrement())
  idReacao      Int
  idLogin       Int
  tbl_login     tbl_login  @relation(fields: [idLogin], references: [idLogin], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_login_reacao_tbl_login1")
  tbl_reacao    tbl_reacao @relation(fields: [idReacao], references: [idReacao], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_login_reacao_tbl_reacao1")

  @@index([idLogin], map: "fk_tbl_login_reacao_tbl_login1_idx")
  @@index([idReacao], map: "fk_tbl_login_reacao_tbl_reacao1_idx")
}

model tbl_meios_de_doacao {
  idmeiosDeDoacao Int     @id @unique(map: "idmeiosDeDoacao_UNIQUE") @default(autoincrement())
  site            String? @db.VarChar(150)
  pix             String? @db.VarChar(100)
  idOng           Int     @unique(map: "idOng_UNIQUE")
  tbl_ong         tbl_ong @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblMeiosDeDoacao_tblOng")
}

model tbl_ong {
  idOng                  Int                      @id @unique(map: "idOng_UNIQUE") @default(autoincrement())
  nome                   String                   @db.VarChar(100)
  descricao              String?                  @db.Text
  numeroDeSeguidores     Int                      @default(0)
  dataDeCriacao          DateTime                 @default(now())
  cnpj                   String                   @unique(map: "cnpj_UNIQUE") @db.VarChar(50)
  foto                   String?                  @db.VarChar(500)
  banner                 String?                  @db.VarChar(500)
  historia               String?                  @db.Text
  qtdDeMembros           Int?
  dataDeFundacao         DateTime?                @default(now())
  idLogin                Int                      @unique
  tbl_login              tbl_login                @relation(fields: [idLogin], references: [idLogin], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_ong_tbl_login1")
  tbl_contato            tbl_contato[]
  tbl_eventos            tbl_eventos[]
  tbl_favoritos          tbl_favoritos[]
  tbl_ong_categoria      tbl_ong_categoria[]
  tbl_ong_estado         tbl_ong_estado[]
  tbl_ong_patrocinadores tbl_ong_patrocinadores[]
  tbl_post               tbl_post[]
  tbl_seguidor           tbl_seguidor[]
  tbl_vagas              tbl_vagas[]
  tbl_meios_de_doacao    tbl_meios_de_doacao[]
  tbl_dados_bancarios    tbl_dados_bancarios[]
  tbl_comentario         tbl_comentario[]
  tbl_post_media         tbl_post_media[]

  @@index([idLogin], map: "fk_tbl_ong_tbl_login1_idx")
}

model tbl_ong_categoria {
  idOngCategoria Int            @id @unique(map: "idtbl-ong_categoria_UNIQUE") @default(autoincrement())
  idOng          Int
  idCategorias   Int
  tbl_categorias tbl_categorias @relation(fields: [idCategorias], references: [idCategorias], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblong_categoria_tbl_categorias1")
  tbl_ong        tbl_ong        @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblong_categoria_tbl_ong1")

  @@index([idCategorias], map: "fk_tblong_categoria_tbl_categorias1_idx")
  @@index([idOng], map: "fk_tblong_categoria_tbl_ong1_idx")
}

model tbl_ong_estado {
  idOngRegiao Int        @id @unique(map: "idtblOngRegiao_UNIQUE") @default(autoincrement())
  idOng       Int
  idEstado    Int
  tbl_estado  tbl_estado @relation(fields: [idEstado], references: [idEstado], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_ong_estado_tbl_estado1")
  tbl_ong     tbl_ong    @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblOngRegiao_tblOng1")

  @@index([idOng], map: "fk_tblOngRegiao_tblOng1_idx")
  @@index([idEstado], map: "fk_tbl_ong_estado_tbl_estado1_idx")
}

model tbl_ong_patrocinadores {
  idOngPatrocinadores Int                @id @unique(map: "idtblOngPatrocinadores_UNIQUE") @default(autoincrement())
  idOng               Int
  idPatrocinadores    Int
  tbl_ong             tbl_ong            @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblOngPatrocinadores_tblOng1")
  tbl_patrocinadores  tbl_patrocinadores @relation(fields: [idPatrocinadores], references: [idPatrocinadores], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblOngPatrocinadores_tblPatrocinadores1")

  @@index([idOng], map: "fk_tblOngPatrocinadores_tblOng1_idx")
  @@index([idPatrocinadores], map: "fk_tblOngPatrocinadores_tblPatrocinadores1_idx")
}

model tbl_patrocinadores {
  idPatrocinadores       Int                      @id @unique(map: "idtblPatrocinio_UNIQUE") @default(autoincrement())
  nome                   String                   @unique(map: "nometblPatrocinadores_UNIQUE") @db.VarChar(45)
  foto                   String?                  @db.Text
  link                   String?                  @db.VarChar(80)
  tbl_ong_patrocinadores tbl_ong_patrocinadores[]
}

model tbl_post {
  idPost              Int                   @id @unique(map: "idtblPosts_UNIQUE") @default(autoincrement())
  descricao           String                @db.Text
  idOng               Int
  createdAt           DateTime              @default(now())
  tbl_post_comentario tbl_post_comentario[]
  tbl_post_reacao     tbl_post_reacao[]
  tbl_post_media      tbl_post_media[]
  tbl_ong             tbl_ong?              @relation(fields: [idOng], references: [idOng])

  @@index([idOng], map: "fk_tblPosts_tblOng1_idx")
}

model tbl_post_comentario {
  idPostComentario Int      @id @unique(map: "idtbl_post_comentario_UNIQUE") @default(autoincrement())
  idcomentario     Int
  idPost           Int
  tbl_post         tbl_post @relation(fields: [idPost], references: [idPost], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_post_comentario_tbl_post1")

  @@index([idcomentario], map: "fk_tbl_post_comentario_tbl_comentario1_idx")
  @@index([idPost], map: "fk_tbl_post_comentario_tbl_post1_idx")
}

model tbl_post_reacao {
  idPostReacao Int        @id @unique(map: "id_post_reacao_UNIQUE") @default(autoincrement())
  idReacao     Int
  idPost       Int
  tbl_post     tbl_post   @relation(fields: [idPost], references: [idPost], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_post_reacao_tbl_post1")
  tbl_reacao   tbl_reacao @relation(fields: [idReacao], references: [idReacao], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_post_reacao_tbl_reacao1")

  @@index([idPost], map: "fk_tbl_post_reacao_tbl_post1_idx")
  @@index([idReacao], map: "fk_tbl_post_reacao_tbl_reacao1_idx")
}

model tbl_reacao {
  idReacao           Int                  @id @unique(map: "idtblCurtida_UNIQUE") @default(autoincrement())
  emoji              String               @db.VarChar(20)
  tbl_login_reacao   tbl_login_reacao[]
  tbl_post_reacao    tbl_post_reacao[]
  tbl_usuario_reacao tbl_usuario_reacao[]
}

model tbl_seguidor {
  idSeguidor  Int         @id @unique(map: "idtblUsuarioOng_UNIQUE") @default(autoincrement())
  idOng       Int
  idUsuario   Int
  tbl_ong     tbl_ong     @relation(fields: [idOng], references: [idOng], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblUsuarioOng_tblOng1")
  tbl_usuario tbl_usuario @relation(fields: [idUsuario], references: [idUsuario], onDelete: NoAction, onUpdate: NoAction, map: "fk_tblUsuarioOng_tblUsuario1")

  @@index([idOng], map: "fk_tblUsuarioOng_tblOng1_idx")
  @@index([idUsuario], map: "fk_tblUsuarioOng_tblUsuario1_idx")
