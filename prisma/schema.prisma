  tbl_vagas      tbl_vagas   @relation(fields: [idVagas], references: [idVagas], onDelete: NoAction, onUpdate: NoAction, map: "fk_tbl_vagas_usuario_tbl_vagas1")

  @@index([idUsuario], map: "fk_tbl_vagas_usuario_tbl_usuario1_idx")
  @@index([idVagas], map: "fk_tbl_vagas_usuario_tbl_vagas1_idx")
}
