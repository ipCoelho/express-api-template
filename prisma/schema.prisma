model tbl_vagas {
  idVagas            Int                  @id @unique(map: "idtbl_vagas_UNIQUE") @default(autoincrement())
  titulo             String               @db.VarChar(200)
  decricao           String               @db.Text
  requisitos         String               @db.Text
  dataDeCriacao      DateTime             @db.Date
